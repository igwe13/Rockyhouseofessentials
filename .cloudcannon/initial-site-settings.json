{
  "ssg": "hugo",
  "mode": "hosted",
  "build": {
    "hugoVersion": "0.128.1",
    "rubyVersion": "2.7.3",
    "nodeVersion": "18",
    "denoVersion": "1.40.2",
    "install_command": "curl -sSOL https://cecil.app/build.sh",
    "build_command": "bash ./build.sh",
    "output_path": "public",
    "preserved_paths": "resources/,.hugo_cache/",
    "environment_variables": [
      {
        "key": "HUGO_CACHEDIR",
        "value": "/usr/local/__site/src/.hugo_cache/"
      }
    ],
    "includeGit": true
  }
}